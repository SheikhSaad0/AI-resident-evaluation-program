{
  "name": "my-new-surgical-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:next dev:ws",
    "dev:next": "next dev",
    "dev:ws": "ts-node --esm scripts/webSocketServer.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "clear-cache": "node scripts/clear-cache.js",
    "dev:clean": "npm run clear-cache && npm run dev"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.9.1",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/text-to-speech": "^6.2.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.12.0",
    "@types/uuid": "^10.0.0",
    "@upstash/qstash": "^2.8.1",
    "@upstash/redis": "^1.35.1",
    "@vercel/analytics": "^1.5.0",
    "@vorlefan/prisma-backup": "^0.3.5",
    "formidable": "^3.5.4",
    "next": "^15.4.1",
    "nodemailer": "^7.0.5",
    "prisma": "^6.12.0",
    "psql": "^0.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^3.1.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/node": "^24",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.38.0"
  }
}
