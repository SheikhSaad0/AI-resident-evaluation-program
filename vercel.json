{
  "functions": {
    "pages/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "includedFiles": [
        "node_modules/fluent-ffmpeg/**",
        "prisma/schema.prisma"
      ]
    }
  },
  "builds": [
    {
      "src": "/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "/api/ffmpeg",
      "use": "vercel-ffmpeg"
    }
  ]
}